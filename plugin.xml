<?xml version="1.0" encoding="UTF-8"?>

<plugin xmlns="http://www.phonegap.com/ns/plugins/1.0"
           id="com.pakhshyaran.kara"
      version="0.1.0">
  <name>Calendar</name>
  <description>Sample PhoneGap Calendar Plugin</description>
  <license>MIT</license>
  <keywords>phonegap,calendar</keywords>


  <js-module src="www/GPSTracking.js" name="GPSTracking">
    <clobbers target="window.gpsTracking" />
  </js-module>

  <!-- android -->
  <platform name="android">
    <config-file target="res/xml/config.xml" parent="/*">
      <feature name="Calendar">
        <param name="android-package" value="com.pakhshyaran.kara.GPSTracking"/>
      </feature>
    </config-file>

    <source-file src="src/android/GPSTracking.java" target-dir="src/com/pakhshyaran/kara" />
  </platform>
</plugin>
